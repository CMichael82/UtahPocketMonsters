<div class="row">
	<button class='right btn grey darken-1'><a class='white-text' href="/logout">Logout</a></button>
	</div>

<div class="container" id="fightBox">
	<div class="row">
		<div class="col m4">
			<div class="card small">
				<div class="card-image">
					<img id='charImage' src='#' id='character'>
					<span class="card title"></span>
				</div>
				<div class="card-content">
					<h5>Life Points: <span id='characterLife'></span></h5>
				</div>
			</div>
		</div>

		<div class="col s4">
			<img src="/images/arrow-2085195_1280.png" height="100%" width="100%" id="arrowImage">
			<br>
			<h6 class='center result'></h6>
			<br>
			<div class="center">
				<button class='btn grey darken-1' id='attackBtn'>Attack</button>
			</div>
		</div>

		<div class="col m4">
			<div class="card small">
				<div class="card-image">
					<img id='monstImage' src="styles/images/Monster1.png">
					<span class="card-title" id="monster"></span>
				</div>
				<div class="card-content">
					<h5>Life Points: <span id='monsterLife'></span></h5>
				</div>
			</div>
		</div>
</div>